<?xml version="1.0" encoding="utf-8"?>
<rotate xmlns:android="http://schemas.android.com/apk/res/android"
    android:fromDegrees="90"
    android:toDegrees="90">


    <shape
        android:shape="line"
        android:tileMode="repeat">
        <stroke
            android:color="#989898"
            android:dashWidth="10dp"
            android:dashGap="5dp"
            android:width="1dp" />
    </shape>

</rotate>
